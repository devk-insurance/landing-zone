{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": [
                "config:Delete*",
                "config:Stop*"
            ],
            "Resource": "*",
            "Condition": {
                "ArnNotLike": {
                  "aws:PrincipalARN":"arn:aws:iam::*:role/AWSCloudFormationStackSetExecutionRole"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": [
                "cloudtrail:Delete*",
                "cloudtrail:Stop*"
            ],
            "Resource": "*",
            "Condition": {
                "ArnNotLike": {
                  "aws:PrincipalARN":"arn:aws:iam::*:role/AWSCloudFormationStackSetExecutionRole"
                }
            }
        }
    ]
}
